<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/_coordinator"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/_app_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/_toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="#FAA916"
            app:popupTheme="@style/AppTheme.PopupOverlay" />
    </com.google.android.material.appbar.AppBarLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">
        <ScrollView
            android:id="@+id/vscroll1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="10dp"
            android:background="#FFF9C4">
            <LinearLayout
                android:id="@+id/linear1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="8dp"
                android:background="#FFFFFF"
                android:orientation="vertical">
                <TextView

                    android:id="@+id/textview3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="Pedigree Dog Registration "
                    android:textSize="25sp"
                    android:textStyle="bold"
                    android:textColor="#40C9A2"
                    android:layout_gravity="center_horizontal|center_vertical" />
                <TextView
                                            android:textStyle="bold"

                    android:id="@+id/textview15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="(Both parents registered)"
                    android:textSize="14sp"
                    android:textColor="#000000"
                    android:layout_gravity="center_horizontal|center_vertical" />
                <LinearLayout
                    android:id="@+id/linear10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:gravity="bottom"
                    android:orientation="horizontal">
                    <TextView

                        android:id="@+id/textview4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="bottom"
                        android:text="Sire's INKC Register Number "
                        android:textSize="16sp"
                        android:textStyle="bold"
                        android:textColor="#000000"
                        android:layout_gravity="bottom" />
                    <TextView
                                                android:textStyle="bold"

                        android:id="@+id/textview20"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="*"
                        android:textSize="16sp"
                        android:textColor="#F44336" />
                </LinearLayout>
                <EditText
                    android:id="@+id/edittext1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="8dp"
                    android:layout_marginTop="7dp"
                    android:background="#E3F2FD"
                    android:textSize="14sp"
                    android:hint="Sire's register number"
                    android:textColor="#000000"
                    android:textColorHint="#607D8B" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:id="@+id/sireset"
                    android:visibility="gone"/>
                <LinearLayout
                    android:id="@+id/linear11"
                    android:layout_marginTop="7dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:textStyle="bold"
                        android:id="@+id/textview5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Dam's INKC Register Number "
                        android:textSize="16sp"
                        android:textColor="#000000" />
                    <TextView
                        android:textStyle="bold"
                        android:id="@+id/textview27"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="8dp"
                        android:text="*"
                        android:textSize="16sp"
                        android:textColor="#F44336" />
                </LinearLayout>
                <EditText
                    android:id="@+id/edittext2"
                    android:layout_marginTop="7dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="8dp"
                    android:hint="Dam's register number"
                    android:background="#E3F2FD"
                    android:textSize="14sp"
                    android:textColor="#000000"
                    android:textColorHint="#607D8B" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:id="@+id/damset"
                    android:visibility="gone"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="6dp"
                    android:paddingRight="6dp"
                    android:paddingBottom="6dp"
                    android:orientation="horizontal">
                    <TextView
                        android:textStyle="bold"
                        android:layout_marginTop="7dp"
                        android:id="@+id/textview16"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="8dp"
                        android:text="Add Second Owner"
                        android:textSize="16sp"
                        android:textColor="#000000" />
                    <TextView
                        android:textStyle="bold"
                        android:id="@+id/textview21"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="*"
                        android:textSize="16sp"
                        android:textColor="#F44336" />
                </LinearLayout>
                <RadioGroup
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:id="@+id/radiogroupgender1"
                    >
                    <RadioButton
                        android:id="@+id/radiobutton11"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="8dp"
                        android:text="Yes"
                        android:textSize="14sp"
                        android:textColor="#000000" />
                    <RadioButton
                        android:id="@+id/radiobutton22"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="8dp"
                        android:text="No"
                        android:textSize="14sp"
                        android:textColor="#000000" />
                </RadioGroup>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >
                    <LinearLayout
                        android:id="@+id/secondeownerlayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="8dp"
                        android:visibility="gone"
                        android:orientation="horizontal">
                        <TextView
                            android:textStyle="bold"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Second owner ID "
                            android:textSize="16sp"
                            android:textColor="#000000" />
                        <TextView
                            android:textStyle="bold"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="*"
                            android:textSize="16sp"
                            android:textColor="#F44336" />
                    </LinearLayout>
                </LinearLayout>
                <EditText
                    android:id="@+id/secondownerid"
                    android:layout_marginTop="7dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="8dp"
                    android:hint="second owner id"
                    android:background="#E3F2FD"
                    android:textSize="14sp"
                    android:visibility="gone"
                    android:textColor="#000000"
                    android:textColorHint="#607D8B" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:id="@+id/secondeownerset"
                    android:visibility="gone"/>
                <LinearLayout
                    android:id="@+id/linear12"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                                                android:textStyle="bold"

                        android:id="@+id/textview17"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="8dp"
                        android:text="Name of Dog"
                        android:textSize="16sp"
                        android:textColor="#000000" />
                    <TextView
                                                android:textStyle="bold"

                        android:id="@+id/textview22"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="*"
                        android:textSize="16sp"
                        android:textColor="#F44336" />
                </LinearLayout>
                <EditText
                    android:layout_marginTop="7dp"
                    android:id="@+id/edittext10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:background="#E3F2FD"
                    android:textSize="14sp"
                    android:hint="Dog Name"
                    android:textColor="#000000"
                    android:textColorHint="#607D8B" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:id="@+id/dognameset"
                    android:visibility="gone"/>
                <LinearLayout
                    android:id="@+id/linear13"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:orientation="horizontal">
                    <TextView
                        android:textStyle="bold"
                        android:id="@+id/textview6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Dog's Date of Birth "
                        android:textSize="16sp"
                        android:textColor="#000000" />
                    <TextView
                                                android:textStyle="bold"

                        android:id="@+id/textview23"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="*"
                        android:textSize="16sp"
                        android:textColor="#F44336" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linear7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:background="#E3F2FD"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:textStyle="bold"
                        android:id="@+id/date"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:background="#E3F2FD"
                        android:textSize="14sp"
                        android:hint="dd-mm-yy"
                        android:textColor="#000000"
                        android:textColorHint="#607D8B" />
                    <ImageView
                        android:id="@+id/date1"
                        android:layout_marginTop="1dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:backgroundTint="#F5F5F5"
                        android:src="@drawable/ic_baseline_calendar_today_24"
                        android:scaleType="center" />
                </LinearLayout>
<!--                <CalendarView-->
<!--                    android:id="@+id/calendarview1"-->
<!--                    android:layout_width="match_parent"-->
<!--                    android:layout_height="match_parent"-->
<!--                    android:padding="8dp" />-->
                <LinearLayout
                    android:id="@+id/linear15"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                                                android:textStyle="bold"

                        android:id="@+id/textview19"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="8dp"
                        android:text="Color &amp; Marking "
                        android:textSize="16sp"
                        android:textColor="#000000" />
                    <TextView
                                                android:textStyle="bold"

                        android:id="@+id/textview28"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="*"
                        android:textSize="16sp"
                        android:textColor="#F44336" />
                </LinearLayout>
                <TextView
                    android:layout_marginTop="7dp"
                    android:id="@+id/spinner1"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:padding="8dp"
                    android:hint="Select Color"
                    android:background="#E3F2FD"
                     />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/colormakingid"
                    android:visibility="gone"/>
                <LinearLayout
                    android:id="@+id/linear4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:background="@android:color/transparent"
                    android:orientation="horizontal">
                    <TextView
                                                android:textStyle="bold"

                        android:id="@+id/textview9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="8dp"
                        android:text="Sex of the Dog"
                        android:textSize="16sp"
                        android:textColor="#000000" />
                    <TextView
                        android:textStyle="bold"
                        android:id="@+id/textview25"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="*"
                        android:textSize="16sp"
                        android:textColor="#F44336" />
                </LinearLayout>
                <RadioGroup
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:id="@+id/radiogroupgender"
                    >
                    <RadioButton
                        android:id="@+id/radiobutton1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="8dp"
                        android:text="Male"
                        android:textSize="14sp"
                        android:textColor="#000000" />
                    <RadioButton
                        android:id="@+id/radiobutton2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="8dp"
                        android:text="Female"
                        android:textSize="14sp"
                        android:textColor="#000000" />
                </RadioGroup>

                <TextView
                    android:textStyle="bold"
                    android:id="@+id/textview7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="Upload your Dog's photographs keep only the third photograph "
                    android:textSize="16sp"
                    android:layout_margin="5dp"
                    android:textColor="#000000" />
                <LinearLayout
                    android:id="@+id/linear8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:orientation="horizontal">
                    <Button
                        android:id="@+id/button1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="8dp"
                        android:backgroundTint="#d4d3d2"
                        android:text="Choose photo"
                        android:textSize="14sp"
                        android:textColor="#000000" />
                    <TextView
                                                android:textStyle="bold"

                        android:id="@+id/textview18"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:padding="8dp"
                        android:text="format: Jpg and Png only\nsize: maximum 200kb"
                        android:textSize="14sp"
                        android:textColor="#757575"
                        android:layout_gravity="center_horizontal|center_vertical" />
                </LinearLayout>
                <ImageView
                    android:layout_width="200dp"
                    android:layout_height="200dp"
                    android:id="@+id/imageselected"
                    android:layout_gravity="center"
                    android:visibility="gone"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/errorimage"
                    android:textColor="#e62315"
                    android:text="This field is required *"
                    android:layout_gravity="center"
                    android:visibility="gone"/>
                <LinearLayout
                    android:id="@+id/linear9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:gravity="center_horizontal|center_vertical"
                    android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/imageview3"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginRight="10dp"
                        android:src="@drawable/pug1"
                        android:scaleType="fitCenter" />
                    <ImageView
                        android:id="@+id/imageview4"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/lab1"
                        android:scaleType="fitCenter" />
                    <ImageView
                        android:id="@+id/imageview5"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="10dp"
                        android:src="@drawable/lab2"
                        android:scaleType="fitCenter" />

                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linear14"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:orientation="horizontal">
                    <TextView
                        android:textStyle="bold"
                        android:id="@+id/textview8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="8dp"
                        android:text="Microchip Required "
                        android:textSize="16sp"
                        android:textColor="#000000" />
                    <TextView
                                                android:textStyle="bold"

                        android:id="@+id/textview26"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="*"
                        android:textSize="18sp"
                        android:textColor="#E53935"
                        android:layout_gravity="center_horizontal|center_vertical" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linear3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#ffffff"
                    android:orientation="horizontal">
<!--                    <RadioButton-->
<!--                        android:id="@+id/radiobutton3"-->
<!--                        android:layout_width="wrap_content"-->
<!--                        android:layout_height="wrap_content"-->
<!--                        android:padding="8dp"-->
<!--                        android:text="Yes, I Require "-->
<!--                        android:textSize="16sp"-->
<!--                        android:textColor="#000000" />-->
<!--                    <RadioButton-->
<!--                        android:id="@+id/radiobutton4"-->
<!--                        android:layout_width="wrap_content"-->
<!--                        android:layout_height="wrap_content"-->
<!--                        android:padding="8dp"-->
<!--                        android:text="No, I dont Require "-->
<!--                        android:textSize="16sp"-->
<!--                        android:textColor="#000000" />-->

                    <RadioGroup
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:id="@+id/microgrop"
                        >
                        <RadioButton
                            android:id="@+id/required"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="8dp"
                            android:text="Yes, I Require "
                            android:textSize="14sp"
                            android:textColor="#000000" />
                        <RadioButton
                            android:id="@+id/norequired"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="8dp"
                            android:text="No, I dont Require "
                            android:textSize="14sp"
                            android:textColor="#000000" />
                    </RadioGroup>
                </LinearLayout>
                <Button
                    android:id="@+id/button2"
                    android:layout_width="110dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="20dp"
                    android:padding="8dp"
                    android:text="Submit"
                    android:textSize="18sp"
                    android:backgroundTint="#40C9A2"
                    android:textStyle="bold"
                    android:textColor="#ffffff"
                    android:layout_gravity="center_horizontal|center_vertical" />
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</androidx.coordinatorlayout.widget.CoordinatorLayout>

